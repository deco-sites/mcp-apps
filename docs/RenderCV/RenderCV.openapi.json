            "type": "string"
          },
          "date": {
            "type": "string",
            "format": "date"
          },
          "awarder": {
            "type": "string"
          },
          "summary": {
            "type": "string"
          }
        }
      },
      "Publication": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "publisher": {
            "type": "string"
          },
          "releaseDate": {
            "type": "string",
