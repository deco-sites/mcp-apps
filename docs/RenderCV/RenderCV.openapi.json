          },
          "issuer": {
            "type": "string"
          },
          "url": {
            "type": "string",
            "format": "uri"
          }
        }
      },
      "ResumeCreateRequest": {
        "type": "object",
        "required": [
          "style",
          "content"
        ],
        "properties": {
