          },
          "entity": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        }
      },
      "Volunteer": {
        "type": "object",
        "properties": {
          "organization": {
            "type": "string"
          },
          "position": {
            "type": "string"
          },
          "url": {
            "type": "string",
