          },
          "url": {
            "type": "string",
            "format": "uri"
          },
          "summary": {
            "type": "string"
          }
        }
      },
      "Language": {
        "type": "object",
        "properties": {
          "language": {
            "type": "string"
          },
          "fluency": {
            "type": "string"
          }
        }
      },
      "Interest": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "keywords": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "Reference": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "reference": {
