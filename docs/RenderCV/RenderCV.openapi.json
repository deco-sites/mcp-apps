{
  "openapi": "3.0.0",
  "info": {
    "title": "RenderCV API",
    "description": "API for interacting with RenderCV service for resume creation and management",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api.rendercv.com/v1"
    }
  ],
  "paths": {
    "/styles": {
      "get": {
        "summary": "List all available styles",
        "operationId": "listStyles",
        "responses": {
          "200": {
            "description": "A list of available styles",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Style"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/resume/{resumeId}": {
      "get": {
        "summary": "Get a resume by ID",
        "operationId": "getResume",
        "parameters": [
          {
            "name": "resumeId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Resume object",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Resume"
                }
              }
            }
          },
          "404": {
            "description": "Resume not found"
          }
        }
      },
      "put": {
        "summary": "Update a resume",
        "operationId": "updateResume",
        "parameters": [
          {
            "name": "resumeId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ResumeUpdateRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Resume updated successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Resume"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "404": {
            "description": "Resume not found"
          }
        }
      },
      "delete": {
        "summary": "Delete a resume",
        "operationId": "deleteResume",
        "parameters": [
          {
            "name": "resumeId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Resume deleted successfully"
          },
          "404": {
            "description": "Resume not found"
          }
        }
      }
    },
    "/resumes": {
      "post": {
        "summary": "Create a new resume",
        "operationId": "createResume",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ResumeCreateRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Resume created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Resume"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input"
          }
        }
      },
      "get": {
        "summary": "List all resumes",
        "operationId": "listResumes",
        "responses": {
          "200": {
            "description": "A list of resumes",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Resume"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/resume/{resumeId}/pdf": {
      "get": {
        "summary": "Generate PDF for a resume",
        "operationId": "generatePdf",
        "parameters": [
          {
            "name": "resumeId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "PDF file",
            "content": {
              "application/pdf": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          },
          "404": {
            "description": "Resume not found"
          }
        }
      }
    },
    "/resume/{resumeId}/html": {
      "get": {
        "summary": "Generate HTML for a resume",
        "operationId": "generateHtml",
        "parameters": [
          {
            "name": "resumeId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "HTML content",
            "content": {
              "text/html": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "404": {
            "description": "Resume not found"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Style": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "preview": {
            "type": "string",
            "format": "uri"
          }
        }
      },
      "Resume": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "style": {
            "type": "string"
          },
          "content": {
            "$ref": "#/components/schemas/ResumeContent"
          }
        }
      },
      "ResumeContent": {
        "type": "object",
        "properties": {
          "basics": {
            "$ref": "#/components/schemas/Basics"
          },
          "work": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Work"
            }
          },
          "education": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Education"
            }
          },
          "skills": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Skill"
            }
          },
          "projects": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Project"
            }
